модуль вызов_функции_присваивание

// 5.6. Вызов функции или метода
/*
Для всех других аргументов, выражение должно быть совместимо по присва-
иванию с типом параметра (§10.2).
*/

тип Супер = класс {}
тип Под = класс(Супер) {}

тип Байты = []Байт
тип Символы = []Символ


фн ф_байт(байт: Байт) {}
фн ф_байт_вектор(байты: Байты) {}
фн ф_под_мб(под: мб Под) {}
фн ф_класс(супер: Супер) {}


вход {
    пусть байт: Байт = 1
    пусть под = Под{}
    пусть байт_вектор = Байты[1]
    ф_байт(байт)
    ф_байт(1)
    ф_байт_вектор(байт_вектор)
    ф_под_мб(под)
    ф_под_мб(пусто)
    ф_класс(под)
}
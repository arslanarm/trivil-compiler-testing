модуль вызов_метода_параметры

// 5.6 Вызов функции или метода
/*
В вызове указывается список аргументов. Число аргументов в списке долж-
но быть равно числу параметров в сигнатуре функции или метода, за исключение
случая, когда в сигнатуре есть вариативный параметр.
*/
тип Класс = класс {}
фн (к: Класс) Функция(ц: Цел64) {}
фн (к: Класс) Функция2(ц: Цел64, ц2: Цел64) {}
фн (к: Класс) Функция3(с: Строка, ц: Цел64, с2: Строка) {}

вход {
    пусть к = Класс{}
    пусть цел64: Цел64 = 1
    пусть строка: Строка = "тест"
    к.Функция(цел64)
    к.Функция2(цел64, цел64)
    к.Функция3(строка, цел64, строка)
}